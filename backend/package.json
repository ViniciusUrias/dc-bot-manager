{
	"name": "discord-bot",
	"version": "1.0.0",
	"description": "",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"dev": "tsx watch --exclude src/discord/users --env-file=.env src/index.ts",
		"start": "node -r tsconfig-paths/register -r module-alias/register dist/index.js",
		"build": "npm run clean && npm install && npm run generate && tsc --build --noCheck  && npm run copy-assets",
		"clean": "rimraf dist",
		"generate": "prisma generate",
		"migrate": "prisma migrate dev",
		"prisma:copy-types": "cpx \"src/generated/**/*.ts\" \"../frontend/src/types/prisma\" ",
		"copy-assets": "copyfiles -u 1 \"prisma/**/*\" \"src/**/*.prisma\" dist/",
		"postinstall": "prisma generate",
		"staging": "cross-env NODE_ENV=staging node dist/index.js",
		"docker:up": "docker-compose up -d",
		"docker:down": "docker-compose down",
		"docker:logs": "docker-compose logs -f",
		"docker:migrate": "docker-compose exec app npx prisma migrate dev",
		"docker:studio": "docker-compose exec app npx prisma studio",
		"docker:test": "docker-compose exec app npm test"
	},
	"keywords": [],
	"prisma": {
		"schema": "prisma/schema.prisma"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.0.1",
		"@fastify/jwt": "^9.1.0",
		"@fastify/multipart": "^9.0.3",
		"@fastify/swagger": "^9.4.2",
		"@fastify/swagger-ui": "^5.2.2",
		"@prisma/client": "^6.5.0",
		"bcrypt": "^5.1.1",
		"better-auth": "^1.2.5",
		"cors": "^2.8.5",
		"cpx2": "^8.0.0",
		"discord.js": "^14.18.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"fastify": "^5.2.2",
		"fastify-better-auth": "^1.0.1",
		"fs-extra": "^11.3.0",
		"jsonwebtoken": "^9.0.2",
		"module-alias": "^2.2.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.1",
		"@types/fs-extra": "^11.0.4",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/node": "^22.13.13",
		"concurrently": "^9.1.2",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"discord-api-types": "^0.37.119",
		"nodemon": "^3.1.9",
		"pino-pretty": "^13.0.0",
		"prisma": "^6.5.0",
		"prisma-generator-typescript-interfaces": "^2.1.0",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.7.1",
		"typescript": "^5.8.2"
	},
	"_moduleAliases": {
		"@": "./dist",
		"@commands": "./dist/commands",
		"@utils": "./dist/utils"
	}
}
